<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fluxmobile.mobilemarketmanager" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.fluxmobile.mobileshopmanager.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                rd
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fluxmobile.mobileshopmanager.AddActivity"
            android:label="@string/title_activity_add"
            android:parentActivityName="com.fluxmobile.mobileshopmanager.MainActivity"
            android:windowSoftInputMode="stateAlwaysVisible" >
        </activity>
        <activity
            android:name="com.fluxmobile.mobileshopmanager.Detail"
            android:label="@string/title_activity_detail"
            android:parentActivityName="com.fluxmobile.mobileshopmanager.MainActivity" >
        </activity>
        <activity
            android:name="com.fluxmobile.mobileshopmanager.ReportActivity"
            android:label="@string/title_activity_report"
            android:parentActivityName="com.fluxmobile.mobileshopmanager.MainActivity">
        </activity>
    </application>

</manifest>
